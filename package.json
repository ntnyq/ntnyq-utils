{
  "name": "ntnyq-utils",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.1",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --stream run build",
    "dev": "pnpm -r --stream run dev",
    "lint": "eslint .",
    "play": "pnpm -C playground run play",
    "prepare": "husky",
    "publish": "pnpm build && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@ntnyq/eslint-config": "^4.0.0-beta.12",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
