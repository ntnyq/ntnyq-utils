{
  "name": "ntnyq-utils",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --stream run build",
    "clean": "rimraf packages/*/dist",
    "dev": "pnpm -r --stream run dev",
    "lint": "eslint .",
    "play": "pnpm -C playground run play",
    "prepare": "husky install",
    "publish": "pnpm build && changeset publish",
    "test": "vitest",
    "typecheck": "vitest typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@ntnyq/eslint-config": "^1.6.1",
    "@ntnyq/prettier-config": "^1.6.1",
    "@types/node": "^18.15.11",
    "eslint": "^8.38.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pnpm": "^8.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.0",
    "vitest": "^0.30.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
